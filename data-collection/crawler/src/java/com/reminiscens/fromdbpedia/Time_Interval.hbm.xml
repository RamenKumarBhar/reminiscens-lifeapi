<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.reminiscens.fromdbpedia.Time_Interval" table="reminiscens.Time_Interval" >
        <id name="time_interval_id" type="int" column="time_interval_id" >
            <generator class="native"/>
        </id>
 
        <property name="start_date">
            <column name="startdate" />
        </property>
        <property name="end_date">
            <column name="enddate"/>
        </property>
        <property name="duration_unit">
            <column name="duration_unit" />
        </property>
        <property name="duration_amount">
            <column name="duration_amount" />
        </property>
        <property name="is_fuzzy">
            <column name="is_fuzzy" />
        </property>
        
        
        <many-to-one name="startdate"
                     column="fuzzy_startdate"
                     cascade="all"  unique="true"
                />
        <many-to-one name="enddate"
                     column="fuzzy_enddate"
                     cascade="all"  unique="true"
                />
    </class>
</hibernate-mapping>
