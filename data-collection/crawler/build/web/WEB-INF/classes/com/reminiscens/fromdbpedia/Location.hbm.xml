<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.reminiscens.fromdbpedia.Location" table="reminiscens.Location" >
        <id name="location_id" type="int" column="location_id" >
            <generator class="native"/>
        </id>
 
        <property name="name">
            <column name="name" />
        </property>
        <property name="accuracy">
            <column name="accuracy"/>
        </property>
        <property name="textual">
            <column name="location_textual"/>
        </property>
        <property name="city">
            <column name="city"/>
        </property>
        <property name="region">
            <column name="region"/>
        </property>
        <property name="country">
            <column name="country"/>
        </property>
        <property name="lat">
            <column name="lat"/>
        </property> 
        <property name="lon">
            <column name="lon"/>
        </property>
        <property name="locale">
            <column name="locale" />
        </property>
        <property name="coordinates_trust">
            <column name="coordinates_trust"/>
        </property> 
    </class>
</hibernate-mapping>
