<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="crowdMemories.Fuzzy_date" table="Fuzzy_Date">
       
        <id type="int">
            <column name="fuzzy_date_id"/>
            <generator class="native"/>
        </id>
        
        <property name="textual_date">
            <column name="textual_date"/>
        </property>
        <property name="exact_date">
            <column name="exact_date"/>
        </property>
        <property name="decade">
            <column name="decade"/>
        </property>
        <property name="year">
            <column name="year" />
        </property>
        <property name="season">
            <column name="season" />
        </property>
        <property name="month">
            <column name="month" />
        </property>
        <property name="day">
            <column name="day" />
        </property>
        <property name="day_name">
            <column name="day_name" />
        </property>
        <property name="day_part">
            <column name="day_part" />
        </property>
        <property name="hour">
            <column name="hour" />
        </property>
        <property name="minute">
            <column name="minute" />
        </property>
        <property name="second">
            <column name="second" />
        </property>
    </class>
</hibernate-mapping>
