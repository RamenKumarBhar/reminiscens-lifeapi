<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="crowdMemories.Album_Media" table="Album_Media">
        <composite-id>
            <key-property name="album_id"  column="album_id"/>
            <key-property name="media_id"  column="media_id"/>
        </composite-id>
    </class>
</hibernate-mapping>
